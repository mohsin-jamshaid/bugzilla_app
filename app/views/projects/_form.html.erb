<%= form_for project, remote: true do |f| %>
	<%= render "devise/shared/error_messages", resource: project %>
	
  <div class="form-group">
  	<%= f.label :title %><br />
  	<%= f.text_field :title , autofocus: true, class: "form-control" %>
	</div>	
	
  <div class="actions">
		<%= f.submit class: "btn btn-primary" %>
	</div>
<% end %>